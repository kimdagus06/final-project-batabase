<body>
    <h1>Upcoming Classes</h1>
    <ul>
        {{#each classes}}
            <li>
                <strong>Teacher: {{user_id}}</strong> - 
                <strong>{{userName}}</strong> is holding this class
                <strong>{{className}}</strong> - {{classType}} - {{startTime}} - {{endTime}} - {{classFormat}} - {{address}} - {{postcode}}
            </li>
        {{else}}
            <li>No upcoming classes available.</li>
        {{/each}}
    </ul>
    <a href="/registerclass">Register New Class</a>
    <a href="/">Back to Home</a>
</body>
